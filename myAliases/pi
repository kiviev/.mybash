
#### alias

        ##### apache
alias www="cd /var/www/html/"
alias apa="cd /etc/apache2/"
alias apaen="cd /etc/apache2/sites-enabled/"
alias apaav="cd /etc/apache2/sites-available/"

alias apasta="sudo apache2 start"
alias apasto="sudo apache2 stop"
alias apasrsta="sudo apache2 restart"
alias apastatus="sudo apache2 status"

#### edit files

alias apaconf="sudo nano /etc/apache2/apache2.conf"